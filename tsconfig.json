{
  "compilerOptions": {
    "module": "commonjs",
    "allowJs": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "suppressImplicitAnyIndexErrors": true,
    "inlineSourceMap": true,
    "target": "es5",
    "lib": [
        "es2015",
        "dom"
    ]
  },
  "exclude": [
    "node_modules",
    "typings"
  ]
}
